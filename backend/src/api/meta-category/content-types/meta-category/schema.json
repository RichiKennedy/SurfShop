{
  "kind": "collectionType",
  "collectionName": "meta_categories",
  "info": {
    "singularName": "meta-category",
    "pluralName": "meta-categories",
    "displayName": "meta-category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "meta_category"
    },
    "img": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "sub_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-category.sub-category",
      "inversedBy": "meta_categories"
    }
  }
}
